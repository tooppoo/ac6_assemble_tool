{
  "name": "@ac6_assemble_tool/web",
  "version": "1.22.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:ci": "cp .env.test .env && vite build",
    "postbuild": "npm run sitemap && npm run purgecss",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "check-types": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check-types:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . && prettier . --check",
    "format": "eslint . --fix && prettier . --write",
    "purgecss": "purgecss --config purgecss.config.cjs",
    "sitemap": "tsx scripts/sitemap-generate.ts",
    "test:integration": "playwright test",
    "test:all": "npm run test:integration && npm run test:unit"
  },
  "devDependencies": {
    "@ac6_assemble_tool/eslint": "1.0.0",
    "@ac6_assemble_tool/spec": "1.0.0",
    "@ac6_assemble_tool/tsconfig": "1.0.0",
    "@fullhuman/postcss-purgecss": "7.0.2",
    "@playwright/test": "1.55.0",
    "@sveltejs/adapter-static": "3.0.8",
    "@sveltejs/kit": "2.37.0",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@sveltestrap/sveltestrap": "7.1.0",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/svelte": "5.2.8",
    "eslint-plugin-svelte": "2.46.1",
    "fake-indexeddb": "6.2.2",
    "globals": "16.3.0",
    "jsdom": "26.1.0",
    "prettier-plugin-svelte": "3.3.3",
    "sass-embedded": "1.86.3",
    "svelte-check": "4.1.6",
    "svelte-sitemap": "2.7.1",
    "vite-bundle-analyzer": "0.17.1"
  },
  "dependencies": {
    "@ac6_assemble_tool/core": "1.0.0",
    "@ac6_assemble_tool/parts": "1.8.1",
    "bootstrap": "5.3.8",
    "bootstrap-icons": "1.11.3",
    "i18next": "24.2.3",
    "svelte": "4.2.20",
    "svelte-i18next": "2.2.2"
  },
  "type": "module"
}
