{
  "name": "@ac6_assemble_tool/web",
  "version": "1.22.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:ci": "dotenv -e .env.test -- vite build",
    "postbuild": "pnpm run sitemap && pnpm run purgecss",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "sync": "dotenv -e .env.test -- svelte-kit sync",
    "sync:watch": "nodemon --watch src/routes --watch src/params --ext svelte,ts --exec \"pnpm run sync\"",
    "check-types": "pnpm run sync && tsc -p ./tsconfig.json --noEmit",
    "check-types:watch": "concurrently \"pnpm run sync:watch\" \"tsc -p ./tsconfig.json --noEmit --watch\"",
    "lint": "concurrently \"pnpm run sync && eslint .\" \"prettier . --check\"",
    "format": "pnpm run sync && eslint . --fix && prettier . --write",
    "purgecss": "purgecss --config purgecss.config.cjs",
    "sitemap": "tsx scripts/sitemap-generate.ts",
    "test:integration": "playwright test",
    "test:all": "pnpm run test:integration && pnpm run test:unit"
  },
  "devDependencies": {
    "@ac6_assemble_tool/eslint": "workspace:1.0.0",
    "@ac6_assemble_tool/spec": "workspace:1.0.0",
    "@ac6_assemble_tool/tsconfig": "workspace:1.0.0",
    "@fullhuman/postcss-purgecss": "7.0.2",
    "@playwright/test": "1.55.0",
    "@sveltejs/adapter-static": "3.0.9",
    "@sveltejs/kit": "2.37.1",
    "@sveltejs/vite-plugin-svelte": "6.2.0",
    "@sveltestrap/sveltestrap": "7.1.0",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/svelte": "5.2.8",
    "concurrently": "9.2.1",
    "dotenv-cli": "10.0.0",
    "eslint-plugin-svelte": "3.11.0",
    "fake-indexeddb": "6.2.2",
    "jsdom": "26.1.0",
    "nodemon": "3.1.10",
    "prettier-plugin-svelte": "3.4.0",
    "sass-embedded": "1.92.0",
    "svelte-check": "4.3.1",
    "svelte-sitemap": "2.7.1",
    "vite-bundle-analyzer": "1.2.3"
  },
  "dependencies": {
    "@ac6_assemble_tool/core": "workspace:1.0.0",
    "@ac6_assemble_tool/parts": "workspace:1.8.1",
    "bootstrap": "5.3.8",
    "bootstrap-icons": "1.13.1",
    "i18next": "25.5.0",
    "svelte": "5.38.6",
    "svelte-i18next": "2.2.2"
  },
  "optionalDependencies": {
    "sass-embedded-darwin-arm64": "1.92.0",
    "sass-embedded-darwin-x64": "1.92.0",
    "sass-embedded-linux-x64": "1.92.0"
  },
  "type": "module"
}
