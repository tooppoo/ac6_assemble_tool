{
  "name": "@ac6_assemble_tool/shared",
  "version": "1.0.0",
  "type": "module",
  "exports": {
    "./*": "./src/*.ts"
  },
  "imports": {
    "#shared/*": "./src/*.ts"
  },
  "scripts": {
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --cache && prettier . --check --cache",
    "format": "eslint . --cache --fix && prettier . --write --cache",
    "check-types": "tsc --noEmit"
  },
  "author": "philomagi <tooppoo56@gmail.com> (https://github.com/tooppoo)",
  "license": "MIT",
  "description": "Shared utilities for AC6 Assemble Tool",
  "devDependencies": {
    "@ac6_assemble_tool/eslint": "workspace:1.0.0",
    "@ac6_assemble_tool/spec": "workspace:1.0.0",
    "@ac6_assemble_tool/tsconfig": "workspace:1.0.0"
  }
}
